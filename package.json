{
  "name": "vtiuse",
  "private": false,
  "version": "1.0.0",
  "description": "Project template based on vue3 + ts",
  "type": "module",
  "license": "MIT",
  "author": "JohnsonXin",
  "bugs": "https://github.com/CodeGetters/vtiuse/issues",
  "keywords": [
    "vue3",
    "TS",
    "template",
    "Pina",
    "vue-router",
    "i18n",
    "vueuse"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:CodeGetters/vtiuse.git"
  },
  "scripts": {
    "dev": "vite --mode development --open",
    "build": "vue-tsc && vite build --mode production",
    "preview": "vite preview",
    "format": "prettier --write ./**/*.{vue,ts,tsx,js,jsx,css,less,scss,json,md}",
    "prepare": "husky install",
    "cz": "git cz",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix"
  },
  "lint-staged": {
    "*.{js,vue,ts,jsx,tsx}": [
      "prettier --write"
    ],
    "*.{html,css,less,scss,md}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "dependencies": {
    "@vueuse/core": "^10.5.0",
    "axios": "^1.6.0",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "vue": "^3.3.7",
    "vue-i18n": "9.6.5",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@types/node": "^20.8.10",
    "@types/nprogress": "^0.2.2",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@unocss/preset-attributify": "^0.57.2",
    "@unocss/preset-icons": "^0.57.2",
    "@unocss/preset-typography": "^0.57.2",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitest/coverage-istanbul": "^0.34.6",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "cz-git": "^1.7.1",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.18.1",
    "happy-dom": "^12.10.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3",
    "sass": "^1.69.5",
    "typescript": "^5.2.2",
    "unocss": "^0.57.2",
    "unplugin-auto-import": "^0.16.7",
    "vite": "^4.5.0",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-vue-devtools": "^1.0.0-rc.5",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.22"
  }
}
